package hello;

import java.time.LocalDateTime;
import java.util.Date;

import org.joda.time.DateTime;
import org.joda.time.format.DateTimeFormat;

public class Record {

    private String cameraid;
    private DateTimeFormat DateTime;
    private String FileName;

    public Record(String cameraid, DateTimeFormat DateTime, String fileName) {
        this.cameraid = cameraid;
        this.DateTime = DateTime;
        this.FileName = fileName;
    }


	public String getId() {
        return cameraid;
    }

    public DateTimeFormat getDt() {
        return DateTime;
    }
    public String getFn() {
        return FileName;
    }
    public void setId(String cameraid) {
    	this.cameraid = cameraid;
    }
    public void setDt(DateTimeFormat DateTime) {
    	this.DateTime = DateTime;
    }
    public void setFn(String fileName) {
    	this.FileName = fileName;
    }
}
